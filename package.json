{
  "name": "ridi-new-entrant-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "npm run lint && tsc && node dist",
    "lint": "tslint -p .",
    "nodemon": "npm run lint && nodemon --watch src --delay 1 --exec 'ts-node' src/index.js",
    "build": "webpack --config webpack.config.js",
    "test": "jest",
    "client": "cross-env NODE_PATH=src webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec NODE_PATH=src ts-node src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "lint-staged": {
    "**.*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix -c tslint.json -p tsconfig.json",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testRegex": "\\.test\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": true
      }
    }
  },
  "repository": {
    "type": "git",
