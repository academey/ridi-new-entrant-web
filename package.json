{
  "name": "ridi-new-entrant-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run lint && tsc && node dist",
    "lint": "tslint -p .",
    "nodemon": "npm run lint && nodemon --watch src --delay 1 --exec 'ts-node' src/index.js",
    "build": "webpack --config webpack.config.js",
    "test": "jest",
    "client": "cross-env NODE_PATH=src/client webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec ts-node src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testRegex": "\\.test\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/academey/ridi-new-entrant-web.git"
  },
  "author": "academey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/academey/ridi-new-entrant-web/issues"
  },
  "homepage": "https://github.com/academey/ridi-new-entrant-web#readme",
  "keywords": [
    "react",
    "es2016",
    "typescript",
    "webpack"
  ],
  "dependencies": {
    "babel-loader": "^8.0.5",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.0.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "mariadb": "^2.0.5",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "~0.12.7",
