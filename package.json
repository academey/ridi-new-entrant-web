{
  "name": "ridi-new-entrant-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p .",
    "test": "NODE_ENV=test jest",
    "build": "webpack --config webpack.config.prod.js -p",
    "client": "cross-env NODE_PATH=src webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec NODE_PATH=src ts-node src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "NODE_PATH=src NODE_ENV=production ts-node -T src/server/index.ts",
    "compose-dev": "docker-compose up",
    "compose-prod": "docker-compose -f docker-compose.prod.yml up --build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "**.*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix -c tslint.json -p tsconfig.json",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testRegex": "\\.test\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/academey/ridi-new-entrant-web.git"
  },
  "author": "academey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/academey/ridi-new-entrant-web/issues"
  },
  "homepage": "https://github.com/academey/ridi-new-entrant-web#readme",
  "keywords": [
    "react",
    "es2016",
    "typescript",
    "webpack"
